{
    "name": "spiral/sapi-bridge",
    "description": "Spiral Framework SAPI bridge",
    "keywords": [
        "SAPI",
        "spiral",
        "sapi-bridge"
    ],
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/sapi-bridge/issues",
        "source": "https://github.com/spiral/sapi-bridge",
        "docs": "https://spiral.dev/docs",
        "forum": "https://forum.spiral.dev",
        "chat": "https://discord.gg/V6EK4he"
    },
    "license": "MIT",
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/spiral"
        }
    ],
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        },
        {
            "name": "Aleksei Gagarin (roxblnfk)",
            "email": "alexey.gagarin@spiralscout.com"
        },
        {
            "name": "Pavel Buchnev (butschster)",
            "email": "pavel.buchnev@spiralscout.com"
        },
        {
            "name": "Maksim Smakouz (msmakouz)",
            "email": "maksim.smakouz@spiralscout.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "nyholm/psr7-server": "^1.0",
        "spiral/boot": "^3.14",
        "spiral/core": "^3.14",
        "spiral/exceptions": "^3.14",
        "spiral/http": "^3.14",
        "psr/http-factory-implementation": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "spiral/dumper": "^3.3",
        "spiral/framework": "^3.14.1",
        "spiral/nyholm-bridge": "^1.2",
        "spiral/router": "^3.14",
        "spiral/testing": "^2.7",
        "vimeo/psalm": "^5.25"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Sapi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/app",
            "Tests\\": "tests/src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "psalm": "vendor/bin/psalm"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "spiral/composer-publish-plugin": false
        }
    },
    "extra": {
        "spiral": {
            "bootloaders": [
                "Spiral\\Sapi\\Bootloader\\SapiBootloader"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
